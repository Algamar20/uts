<body>
    <script>
        var x =2;
        document.write("Looping dimulai dari : <br/>");
        while(x < 22 )
        {
            x = x+3;
            if (x == 4)
            { 
                continue;//skill rest of the loop body
            }
            document.write(+x+ "<br/>");
            x++;
        }
        document.write("looping selesai! <br/>");
    </script>
</body>
